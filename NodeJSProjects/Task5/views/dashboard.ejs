<!DOCTYPE html>
<html>
<head>
  <title>User Dashboard</title>
  <link rel="stylesheet" href="/style.css">
</head>
<body>
  <h2>User Dashboard</h2>
  <table>
    <thead>
      <tr>
        <th>Name</th><th>Email</th><th>Age</th><th>Gender</th><th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% users.forEach(user => { %>
        <tr>
          <td><%= user.username %></td>
          <td><%= user.email %></td>
          <td><%= user.age %></td>
          <td><%= user.gender %></td>
          <td>
            <a href="/edit/<%= user.id %>" class="btn btn-edit">Edit</a>
            <a href="/delete/<%= user.id %>" class="btn btn-delete" onclick="return confirm('Are you sure?')">Delete</a>
          </td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
